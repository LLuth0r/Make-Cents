 <div className=\"dollarSaved\">\n          <div className='header'>\n           <Link to='/'>\n              <img className='cents-logo' src=\"https://res.cloudinary.com/decd84s0g/image/upload/v1607529656/Make%20Cents/vectorstock_22357186.png\" />\n              </Link> \n          <h1 className=\"app-title\">100 Pennies Make Cent$</h1>\n          </div>\n          <div>\n            <form onSubmit={this.handleSubmit}>\n              <input\n                type='number'\n                name='years'\n                value={this.state.years}\n                onChange={(e) => {\n                  this.setState({ years: Number(e.target.value) })\n                }} />\n              <input type='submit' />\n            </form>\n          </div>\n         <DollarChart />\n          <button className='addYear'>+ Year</button>\n          <button className='minusYear'>- Year</button>\n          <h3>Total $aved</h3>\n        </div>\n      );\n    }\n  }\n\nexport default withRouter(DollarSaved);\n",["67","68"],"/Users/mikepakulak/General_Assembly/SEI/Garnet/Unit_2/project_2/make-cents/src/components/AddExpense.jsx",["69","70"],"import axios from 'axios';\nimport { budgetURL, incomeURL, config } from '../services';\nimport React, { useState } from 'react';\nimport { useParams } from 'react-router-dom';\nimport './AddExpense.css';\nimport ExpenseChart from './ExpenseChart';\nimport { Link } from 'react-router-dom';\n\nconst AddExpense = (props) => {\n  const [item, setItem] = useState('');\n  const [cost, setCost] = useState('');\n  const [expense, setExpense] = useState(false);\n  const params = useParams();\n  console.log(params);\n\n  async function handleExpenseSubmit(e) {\n    e.preventDefault();\n    let data = {\n      item,\n      expense_cost: cost,\n      expense,\n    };\n\n    await axios.post(budgetURL, { fields: data }, config);\n    props.setToggleFetch((prev) => !prev);\n  };\n  \n  const handleCheckBox = (e) => {\n    setExpense(!expense)\n  }\n\n  console.